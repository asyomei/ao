{
  "arrowParens": "avoid",
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^#/",
    "^\\."
  ],
  "importOrderTypeScriptVersion": "5.0.0",
  "plugins": [
    "prettier-plugin-astro",
    "@ianvs/prettier-plugin-sort-imports"
  ]
}
